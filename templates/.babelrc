{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "entry"
        // "modules": "umd"
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-transform-modules-commonjs",
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": 3,
        "helpers": true,
        "regenerator": true,
        "useESModules": false
      }
    ],
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    "@babel/plugin-transform-typescript",
    "@babel/plugin-proposal-class-properties"
  ],
  // "minified": true,
  "comments": false,
  "ignore": []
}
